{
  "name": "ap-scoring-guide-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "keywords": [],
  "author": "",
  "license": "MPL-2.0",
  "scripts": {
    "webpack": "cross-env NODE_ENV=development webpack --config webpack.config.js --env.dev",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js --env.prod ",
    "webpack-vendor": "cross-env NODE_ENV=development webpack --config webpack.config.vendor.js --env.dev",
    "webpack-vendor:prod": "cross-env NODE_ENV=production webpack --config webpack.config.vendor.js --env.prod",
    "build": "npm run webpack-vendor && npm run webpack",
    "build:prod": "npm run webpack-vendor:prod && npm run webpack:prod",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch": "npm run webpack-vendor && npm run webpack -- --watch",
    "prettier": "prettier --write client/src/**/*.ts*"
  },
  "dependencies": {
    "@osu-cass/sb-components": "^1.4.2",
    "@sbac/sbac-ui-kit": "github:osu-cass/sbac-global-ui-kit#develop",
    "bootstrap": "^3.3.7",
    "event-source-polyfill": "0.0.11",
    "font-awesome": "~4.7.0",
    "jquery": "3.2.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-modal": "^3.1.11",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "typeface-pt-sans-caption": "0.0.44",
    "typeface-pt-serif": "0.0.44",
    "typeface-pt-serif-caption": "0.0.44",
    "es6-promise": "4.1.1"
  },
  "devDependencies": {
    "@osu-cass/tslint-config": "^1.0.4",
    "@types/enzyme": "^3.1.3",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/history": "^4.6.2",
    "@types/jest": "~21.1.2",
    "@types/jquery": "3.2.12",
    "@types/react": "^16.0.21",
    "@types/react-dom": "^16.0.2",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-router": "^4.0.17",
    "@types/react-router-dom": "^4.2.1",
    "@types/webpack-env": "^1.13.2",
    "copy-webpack-plugin": "^4.3.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.4",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "~21.2.1",
    "jest-cli": "^21.2.1",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "less": "2.7.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.1.3",
    "source-map-loader": "0.2.1",
    "style-loader": "^0.19.0",
    "ts-jest": "21.2.4",
    "ts-loader": "^3.2.0",
    "tslint": "^5.9.1",
    "tslint-language-service": "^0.9.8",
    "typescript": "^2.6.2",
    "url-loader": "^0.5.9",
    "webpack": "3.5.5",
    "webpack-dev-server": "^2.9.4"
  },
  "lint-staged": {
    "{src}/**/*{.tsx,.ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/setupTests.ts",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*(test)\\.(ts|tsx?)$",
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "!src/**/__tests__/*",
      "!src/index.tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.json",
        "skipBabel": true
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "identity-obj-proxy",
      "^.+\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
    }
  }
}
