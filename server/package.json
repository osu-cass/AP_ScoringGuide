  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc && npm run less",
    "start": "node dist/Server.js",
    "test": "jest",
    "dev": "npm run build && npm start",
    "coverage": "jest --coverage",
    "prettier": "prettier --write server/src/**/*.ts*",
    "less": "lessc ./node_modules/@osu-cass/sb-components/lib/Assets/Styles/pdf.less public/pdf/pdf.css"
  },
  "keywords": [],
  "author": "",
  "license": "MPL-2.0",
  "dependencies": {
    "@osu-cass/sb-components": "^1.4.2",
    "@sbac/sbac-ui-kit": "github:osu-cass/sbac-global-ui-kit#develop",
    "@types/body-parser": "^1.16.8",
    "@types/morgan": "^1.7.35",
    "@types/react-modal": "^3.1.2",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "4.15.4",
    "font-awesome": "~4.7.0",
    "jquery": "3.2.1",
    "morgan": "^1.9.0",
    "node": "^9.2.0",
    "puppeteer": "0.9.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-modal": "^3.1.11",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "request": "2.81.0",
    "typeface-pt-sans-caption": "0.0.44",
    "typeface-pt-serif": "0.0.44",
    "typeface-pt-serif-caption": "0.0.44",
    "wkhtmltopdf": "0.3.4"
  },
  "devDependencies": {
    "@osu-cass/tslint-config": "^1.0.4",
    "@types/enzyme": "^3.1.2",
    "@types/enzyme-adapter-react-16": "^1.0.0",
    "@types/express": "4.0.37",
    "@types/jest": "^21.1.5",
    "@types/jquery": "3.2.12",
    "@types/node": "8.0.25",
    "@types/react": "16.0.21",
    "@types/react-dom": "16.0.2",
    "@types/react-router": "^4.0.17",
    "@types/request": "2.0.3",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "15.6.1",
    "ts-jest": "21.2.4",
    "typescript": "2.6.2",
    "tslint": "^5.9.1",
    "tslint-language-service": "^0.9.8",
    "prettier": "^1.9.2",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "less": "2.7.3"
  },
  "lint-staged": {
    "{src}/**/*{.tsx,.ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/setupTests.ts",
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx?)$",
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "!src/**/__tests__/*",
      "!src/index.tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ]
  }
}
